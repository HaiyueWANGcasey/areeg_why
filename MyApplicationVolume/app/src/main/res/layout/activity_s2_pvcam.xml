<!--<?xml version="1.0" encoding="utf-8"?>-->
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/drawer_layout1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".core.S2Activity"
    android:orientation="vertical"
    android:fitsSystemWindows="true">

    <com.google.android.material.appbar.AppBarLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:theme="@style/AppTheme.AppBarOverlay">

        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbar2"
            android:layout_width="match_parent"
            android:layout_height="?attr/actionBarSize"
            android:background="?attr/colorPrimary"
            app:popupTheme="@style/AppTheme.PopupOverlay" />


    </com.google.android.material.appbar.AppBarLayout>

    <FrameLayout
        android:id="@+id/container1"
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="match_parent"
            android:layout_height="374dp"
            android:layout_marginRight="10dp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            app:srcCompat="@drawable/close_icon" />

        <com.penglab.hi5.basic.utils.view.ImageButtonExt
            android:id="@+id/pv_right"
            android:layout_width="35dp"
            android:layout_height="55dp"
            android:layout_marginTop="500dp"
            android:src="@drawable/ic_chevron_left_black_24dp"
            android:layout_marginStart="50dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" />

        <com.penglab.hi5.basic.utils.view.ImageButtonExt
            android:id="@+id/pv_left"
            android:layout_width="35dp"
            android:layout_height="55dp"
            android:layout_marginStart="150dp"
            android:layout_marginTop="500dp"
            android:src="@drawable/ic_chevron_right_black_24dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <com.penglab.hi5.basic.utils.view.ImageButtonExt
            android:id="@+id/pv_top"
            android:layout_width="35dp"
            android:layout_height="55dp"
            android:layout_marginRight="10dp"
            android:layout_marginLeft="110dp"
            android:layout_marginTop="450dp"
            android:src="@drawable/ic_expand_less_black_24dp"
            android:layout_marginStart="100dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" />

        <com.penglab.hi5.basic.utils.view.ImageButtonExt
            android:id="@+id/pv_down"
            android:layout_width="35dp"
            android:layout_height="55dp"
            android:layout_marginTop="550dp"
            android:layout_marginStart="100dp"
            android:src="@drawable/ic_expand_more_black_24dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <com.penglab.hi5.basic.utils.view.ImageButtonExt
            android:id="@+id/pv_zp"
            android:layout_width="35dp"
            android:layout_height="55dp"
            android:layout_marginTop="450dp"
            android:layout_marginStart="300dp"
            android:src="@null"
            android:text="@string/MoveZdown"
            android:textColor="#cc0000"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <com.penglab.hi5.basic.utils.view.ImageButtonExt
            android:id="@+id/pv_zd"
            android:layout_width="35dp"
            android:layout_height="55dp"
            android:layout_marginTop="500dp"
            android:layout_marginStart="300dp"
            android:src="@null"
            android:text="@string/MoveZup"
            android:textColor="#cc0000"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />



    </FrameLayout>






</LinearLayout>


